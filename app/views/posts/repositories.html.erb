<h4 class="row">
  <% @pages.times do |x| %>
    <%= link_to "Page #{x + 1}", page: x + 1 %> ||
  <% end %>
 </h4>

<% @repositories.each do |repo| %>
<h5 class="row">Repo Name: <%= link_to repo.name, repo.html_url %> </h5>
<h5 class="row">Description: <%= repo.description %> </h5>
<h5 class="row"><%= repo.created_at.strftime("Created: %b %d, %Y") %> </h5>
<h5 class="row"><%= repo.updated_at.strftime("Last updated: %b %d, %Y") %></h5>
<h5 class="row">Stars:<%= repo.watchers %> </h5>
<% end %>
